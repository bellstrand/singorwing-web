{
	"extends": "./node_modules/aurelia-tools/.eslintrc.json",
	"rules": {
		"indent": [2, "tab", {
			"SwitchCase": 1
		}],
		"keyword-spacing": [2, {
			"before": true,
			"after": true,
			"overrides": {
				"if": { "after": false },
				"for": { "after": false },
				"while": { "after": false },
				"switch": { "after": false },
				"catch": { "after": false }
			}
		}],
		"no-unused-vars": 1,
		"semi": 1,
		"no-extra-semi": 1,
		"comma-dangle": 1,
		"padded-blocks": 1,
		"space-before-blocks": 1,
		"default-case": 0
	}
}
